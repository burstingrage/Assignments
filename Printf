 *
 *This program is for displaying Printf examples, part of 3.1 class exercise
 */
public class Printf {
	public static void main(String[] args) {
		int n1 = 5;
		char letter = 'A';
		String Fruit = "peach";
		double n2, n3;
		n2 = 4200;
		n3 = 50;
		
		 //PrintF examples below
		
		//Printf decimal interger
			System.out.printf("Printf using Decimal Interger is: %d2", n1);
			System.out.println();
			
			//Printf String
			System.out.printf("Printf using String is: %s", Fruit);
			System.out.println();
			
			//Printf character
			System.out.printf("Printf using Character is: %c", letter);
			System.out.println();
			
			//Printf exponential floating-point number
			System.out.printf("Printf using Exponential floating-point number is: %1.3e", n3);
			System.out.println();
			
			//Printf floating-point number
			System.out.printf("Printf using Floating-point number is: %1.2f", n2);
		
		}

	}

